    "def preprocess_xray_image(img_array):\n",
    "    \"\"\"\n",
    "    Pr√©paration d'une radiographie pour l'analyse IA\n",
    "    \n",
    "    √âtapes du preprocessing m√©dical :\n",
    "    1. Normalisation des intensit√©s\n",
    "    2. Centrage de l'image\n",
    "    3. Redimensionnement standard\n",
    "    4. Conversion en format PyTorch\n",
    "    \"\"\"\n",
    "    print(\"‚öôÔ∏è Pr√©paration de la radiographie pour l'IA...\")\n",
    "    \n",
    "    # √âtape 1 : Normalisation (essentielle en imagerie m√©dicale)\n",
    "    print(\"   1. Normalisation des intensit√©s...\")\n",
    "    if img_array.max() > 1:\n",
    "        img_normalized = xrv.datasets.normalize(img_array, 255)  # Normalisation 0-255 vers 0-1\n",
    "    else:\n",
    "        img_normalized = img_array\n",
    "    \n",
    "    # √âtape 2 : Conversion en format mono-canal si n√©cessaire\n",
    "    print(\"   2. Conversion en niveaux de gris...\")\n",
    "    if len(img_normalized.shape) == 3:\n",
    "        img_gray = img_normalized.mean(axis=2)  # Moyenne des canaux RGB\n",
    "    else:\n",
    "        img_gray = img_normalized\n",
    "    \n",
    "    # √âtape 3 : Ajout dimension canal\n",
    "    img_channel = img_gray[None, ...]  # Ajout dimension canal\n",
    "    \n",
    "    # √âtape 4 : Transformations sp√©cialis√©es radiologie\n",
    "    print(\"   3. Application des transformations radiologiques...\")\n",
    "    img_transformed = transform(img_channel)\n",
    "    \n",
    "    # √âtape 5 : Conversion PyTorch\n",
    "    print(\"   4. Conversion en tenseur PyTorch...\")\n",
    "    img_tensor = torch.from_numpy(img_transformed).float()\n",
    "    \n",
    "    # Ajout dimension batch\n",
    "    img_batch = img_tensor.unsqueeze(0)  # [1, 1, 224, 224]\n",
    "    \n",
    "    print(f\"‚úÖ Image pr√©par√©e ! Dimensions finales : {img_batch.shape}\")\n",
    "    print(f\"   Format : [batch_size, canaux, hauteur, largeur]\")\n",
    "    \n",
    "    return img_batch, img_transformed\n",
    "\n",
    "# Test avec notre exemple normal\n",
    "print(\"üî¨ Test du preprocessing sur radiographie normale...\")\n",
    "test_img = examples[\"normal\"]\n",
    "processed_img, display_img = preprocess_xray_image(test_img)\n",
    "\n",
    "# Visualisation du preprocessing\n",
    "fig, axes = plt.subplots(1, 3, figsize=(15, 5))\n",
    "fig.suptitle(\"√âtapes du Preprocessing Radiologique\", fontsize=16, fontweight='bold')\n",
    "\n",
    "# Image originale\n",
    "axes[0].imshow(test_img, cmap='gray')\n",
    "axes[0].set_title(f\"Originale\\n{test_img.shape}\", fontweight='bold')\n",
    "axes[0].axis('off')\n",
    "\n",
    "# Image apr√®s transformations\n",
    "axes[1].imshow(display_img[0], cmap='gray')  # Premier canal\n",
    "axes[1].set_title(f\"Transform√©e\\n{display_img.shape}\", fontweight='bold')\n",
    "axes[1].axis('off')\n",
